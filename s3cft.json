{
	 "AWSTemplateFormatVersion": "2010-09-09",
	 "Description": "Simple S3 Bucket",
		"Parameters": { 
			"LambdaFunctionName": {
			"Description": "Lambda Function name",
			"Type": "String"
			}
		}
		 "Resources": {
			 "mys3bucket": {
				 "Type": "AWS::S3::Bucket",
					 "Properties":{
						"BucketName": {"Ref":"LambdaFunctionName"}
					}
				}
			}
}
